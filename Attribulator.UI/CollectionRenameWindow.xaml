<Window x:Class="Attribulator.UI.CollectionRenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Attribulator.UI"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="New collection name" Height="170" Width="370" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="New name:" HorizontalAlignment="Left" Margin="8,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="NameTextBox" HorizontalAlignment="Left" Margin="10,41,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="337" Height="22"/>
        <Button Content="Cancel" HorizontalAlignment="Right"  VerticalAlignment="Bottom" Height="28" Width="77" Click="Button_Cancel_Click" Margin="0,0,10,10"/>
        <Button Content="OK" HorizontalAlignment="Right"  VerticalAlignment="Bottom" Height="28" Width="77" Click="Button_Ok_Click" Margin="0,0,104,10"/>
    </Grid>
</Window>
