<Window x:Class="Attribulator.UI.NewNodeNameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Attribulator.UI"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="New node name" Height="151" Width="341" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="New node name:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="NameTextBox" HorizontalAlignment="Left" Margin="16,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="315" Height="18"/>
        <Button Content="Cancel" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Height="30" Width="75" Click="Button_Cancel_Click"/>
        <Button Content="OK" HorizontalAlignment="Right" Margin="0,0,104,10" VerticalAlignment="Bottom" Height="30" Width="75" Click="Button_Ok_Click"/>
    </Grid>
</Window>
